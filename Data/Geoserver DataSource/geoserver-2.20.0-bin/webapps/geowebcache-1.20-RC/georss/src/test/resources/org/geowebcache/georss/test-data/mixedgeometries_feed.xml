  <entry>
    <title>Point sample</title>
    <link href="http://www.georss.org/example" />
    <id>point-1</id>
    <updated>2005-08-17T07:02:32Z</updated>
    <content>a sample geometry</content>
    <georss:where>
      <gml:Point>
        <gml:pos>45.256 -110.45</gml:pos>
      </gml:Point>
    </georss:where>
  </entry>
  <entry>
    <title>MultiPoint sample</title>
    <link href="http://www.georss.org/example" />
    <id>multipoint-1</id>
    <updated>2005-08-17T07:02:33Z</updated>
    <content>a sample geometry</content>
    <georss:where>
      <gml:MultiPoint>
        <gml:pointMembers>
          <gml:Point>
            <gml:pos>45.256 -110.45</gml:pos>
          </gml:Point>
        </gml:pointMembers>
      </gml:MultiPoint>
    </georss:where>
  </entry>
  <entry>
    <title>Polygon sample</title>
    <link href="http://www.georss.org/example" />
    <id>uno</id>
    <updated>2010-08-17T07:02:32Z</updated>
    <content>a sample geometry</content>
    <georss:where>
      <gml:Polygon>
        <gml:exterior>
          <gml:LinearRing>
            <gml:posList>45.256 -110.45 46.46 -109.48 43.84 -109.86
              45.8 -109.2 45.256 -110.45</gml:posList>
          </gml:LinearRing>
        </gml:exterior>
      </gml:Polygon>
    </georss:where>
  </entry>
  <entry>
    <title>MultiPolygon sample</title>
    <link href="http://www.georss.org/example" />
    <id>multipolygon-1</id>
    <updated>2005-08-15T07:02:32Z</updated>
    <content>a sample geometry</content>
    <georss:where>
      <gml:MultiPolygon srsName="EPSG:4269">
        <gml:polygonMember>
          <gml:Polygon srsName="EPSG:4269">
            <gml:exterior>
              <gml:LinearRing srsName="EPSG:4269">
                <gml:pos>-73.30847 44.1808</gml:pos>
                <gml:pos>-73.30882 44.181347</gml:pos>
                <gml:pos>-73.30941 44.18313</gml:pos>
                <gml:pos>-73.30817 44.17996</gml:pos>
                <gml:pos>-73.308266 44.18003</gml:pos>
                <gml:pos>-73.30847 44.1808</gml:pos>
              </gml:LinearRing>
            </gml:exterior>
          </gml:Polygon>
        </gml:polygonMember>
        <gml:polygonMember>
          <gml:Polygon srsName="EPSG:4269">
            <gml:exterior>
              <gml:LinearRing srsName="EPSG:4269">
                <gml:pos>-73.320496 44.22399</gml:pos>
                <gml:pos>-73.32173 44.224445</gml:pos>
                <gml:pos>-73.32402 44.22511</gml:pos>
                <gml:pos>-73.31776 44.222664</gml:pos>
                <gml:pos>-73.31928 44.223648</gml:pos>
                <gml:pos>-73.320496 44.22399</gml:pos>
              </gml:LinearRing>
            </gml:exterior>
          </gml:Polygon>
        </gml:polygonMember>
      </gml:MultiPolygon>
    </georss:where>
  </entry>

  <entry>
    <title>LineString sample</title>
    <link href="http://www.georss.org/example" />
    <id>linestring-1</id>
    <updated>2009-08-17T07:02:32Z</updated>
    <content>a sample geometry</content>
    <georss:where>
      <gml:LineString>
        <gml:posList>40.73615 -73.991309 40.736381 -73.991199</gml:posList>
      </gml:LineString>
    </georss:where>
  </entry>

  <entry>
    <title>MultiLineString sample</title>
    <link href="http://www.georss.org/example" />
    <id>multilinestring-1</id>
    <updated>2009-09-17T07:02:32Z</updated>
    <content>a sample geometry</content>
    <georss:where>
      <gml:MultiLineString srsName="urn:x-ogc:def:crs:EPSG:4326">
        <gml:lineStringMember>
          <gml:LineString>
            <gml:posList>40.73615 -73.991309 40.736381 -73.991199</gml:posList>
          </gml:LineString>
        </gml:lineStringMember>
        <gml:lineStringMember>
          <gml:LineString>
            <gml:posList>40.73158 -73.999559 40.732188 -73.999079</gml:posList>
          </gml:LineString>
        </gml:lineStringMember>
      </gml:MultiLineString>
    </georss:where>
  </entry>

