<?xml version="1.0" encoding="utf-8"?>
<gwcConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://geowebcache.org/schema/1.20.0 http://geowebcache.org/schema/1.20.0/geowebcache.xsd" xmlns="http://geowebcache.org/schema/1.20.0">
  <version>1.20.0</version>
  <backendTimeout>120</backendTimeout>
  <serviceInformation>
    <title>GeoWebCache</title>
    <description>GeoWebCache is an advanced tile cache for WMS servers. It supports a large variety of protocols and
      formats, including WMS-C, WMTS, KML, Google Maps and Virtual Earth.
    </description>
    <fees>NONE</fees>
    <accessConstraints>NONE</accessConstraints>
  </serviceInformation>
  <layers>
    <mbtilesLayer>
      <tilesPath>world_lakes.mbtiles</tilesPath>
    </mbtilesLayer>
    <mbtilesLayer>
      <tilesPath>manypoints_test.mbtiles</tilesPath>
      <tileSize>256</tileSize>
      <name>testName</name>
      <metaInformation>
        <title>TestTitle</title>
        <description>TestDescription</description>
      </metaInformation>

    </mbtilesLayer>
  </layers>
</gwcConfiguration>
