<?xml version="1.0" encoding="UTF-8"?>
<StyledLayerDescriptor version="1.0.0"
    xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd"
    xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc"
    xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">   <!--
    a Named Layer is the basic building block of an SLD document -->
    <NamedLayer>
        <Name>njtech</Name>
        <UserStyle>
            <Name>njtech</Name>
            <FeatureTypeStyle>
                <Name>group0</Name>
                <FeatureTypeName>Feature</FeatureTypeName>
                <SemanticTypeIdentifier>generic:geometry</SemanticTypeIdentifier>
                <SemanticTypeIdentifier>simple</SemanticTypeIdentifier>
                <Rule>
                    <Name>default rule</Name>
                    <PolygonSymbolizer>
                        <Fill>
                            <CssParameter name="fill">#FFEABE</CssParameter>
                            <CssParameter name="fill-opacity">0.5</CssParameter>
                        </Fill>
                        <Stroke>
                            <CssParameter name="stroke">#A6761D</CssParameter>
                        </Stroke>
                    </PolygonSymbolizer>
                </Rule>
            </FeatureTypeStyle>
        </UserStyle>
    </NamedLayer>
</StyledLayerDescriptor>